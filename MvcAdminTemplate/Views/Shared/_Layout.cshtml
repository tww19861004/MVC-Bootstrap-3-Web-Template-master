<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My MVC Admin Template</title>
    @Styles.Render("~/Content/Theme")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body class="bootstrap-admin-with-small-navbar">
   
    <!-- main / large navbar -->
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="row">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".main-navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/">AI代码机器人</a>
                </div>
                <div class="collapse navbar-collapse main-navbar-collapse">                    
                    <div class="collapse navbar-collapse bootstrap-admin-navbar-sm">
                        <ul class="nav navbar-nav navbar-right ">
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-hover="dropdown">生成数据库脚本 <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li role="presentation" class="dropdown-header">非关系型数据库</li>
                                    <li><a href="/Statistics">SQLite</a></li>
                                    <li><a href="/Forms">Another action</a></li>
                                    <li><a href="/Calendar">Something else here</a></li>
                                    <li role="presentation" class="divider"></li>
                                    <li role="presentation" class="dropdown-header">关系型数据库</li>
                                    <li><a href="#">SQL Server</a></li>
                                    <li><a href="#">One more separated link</a></li>
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-hover="dropdown">缓存 <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Memcached</a></li>                                    
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-hover="dropdown">ORM <b class="caret"></b></a>
                                <ul class="dropdown-menu">      
                                    <li><a href="#">NPoco</a></li>                              
                                    <li><a href="#">MicroOrm.Net</a></li>
                                    <li><a href="#">EF</a></li>
                                    <li><a href="#">NHibernate</a></li>
                                    <li><a href="#">Dapper.Net</a></li>                                    
                                    <li><a href="#">Simple.Data</a></li>  
                                </ul>
                            </li>
                            <li>
                                <a href="#">Nancy <i class="glyphicon glyphicon-bell"></i></a>
                            </li>
                            <li>
                                <a href="#">Protobuf-net <i class="glyphicon glyphicon-leaf"></i></a>
                            </li>       
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-hover="dropdown">杂货机器人 <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">在线JSON序列化</a></li>
                                    <li><a href="#">CSV转table</a></li>
                                </ul>
                            </li>  
                            <li>
                                <a href="/Account/Register">注册 <i class="glyphicon glyphicon-user"></i></a>
                            </li>  
                            <li>
                                <a href="/Account/Login">登陆 <i class="glyphicon glyphicon-home"></i></a>
                            </li>                     
                            @*@Html.Partial("_LoginPartial")*@
                        </ul>
                    </div>
                </div><!-- /.navbar-collapse -->
            </div>
        </div><!-- /.container -->
    </nav>
    <div class="container">
        <div class="row">
            @RenderBody()
        </div>
        <div class="row">
            <hr>
            <footer role="contentinfo">
                <p>&copy;  @DateTime.Now.Year -  <a href="#" target="_blank">tangweiwei</a></p>
            </footer>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-45396411-1', 'azurewebsites.net');
        ga('send', 'pageview');

    </script>
</body>
</html>
